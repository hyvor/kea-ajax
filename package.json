{
    "name": "kea-ajax",
    "version": "1.0.0",
    "type": "commonjs",
    "main": "src/index.ts",
    "types": "src/index",
    "peerDependencies": {
        "kea": ">= 3"
    },
    "devDependencies": {
        "@babel/plugin-transform-modules-commonjs": "^7.18.0",
        "@babel/preset-env": "^7.18.0",
        "@babel/preset-typescript": "^7.17.12",
        "@types/react": "^18.0.9",
        "@wessberg/ts-clone-node": "^0.3.19",
        "babel-jest": "^28.1.0",
        "jest": "^28.1.0",
        "kea-typegen": "^3.0.0",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "typescript": "^4.6.4"
    },
    "scripts": {
        "build": "tsc -b",
        "watch": "tsc -w",
        "test": "jest",
        "start:kea": "kea-typegen watch --write-paths",
        "build:kea": "kea-typegen write"
    },
    "babel": {
        "env": {
            "test": {
                "plugins": [
                    "@babel/plugin-transform-modules-commonjs"
                ]
            }
        }
    }
}
